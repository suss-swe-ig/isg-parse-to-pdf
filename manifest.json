{
    "name": "ISG-to-PDF",
    "description": "Parses the SUSS iStudyGuide to PDFt",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": ["main.js"],
            "matches": [
                "https://isg.suss.edu.sg/study-guide/*"
            ]
        }
    ]
  }